{
  "expo": {
    "name": "AnyGoodExpo",
    "slug": "anygoodexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/drawable/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/drawable/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/drawable/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.example.anygoodexpo",
      "jsEngine": "jsc"
    },
    "web": {
      "favicon": "./assets/drawable/ic_settings.png"
    }
  }
}

