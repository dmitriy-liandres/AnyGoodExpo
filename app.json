{
  "expo": {
    "name": "AnyGood",
    "slug": "anygood",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/drawable/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/drawable/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.anygood",
	  "appStoreUrl": "https://apps.apple.com/app/anygood/id6743716669",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/drawable/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.anygood",
      "jsEngine": "jsc",
	  "playStoreUrl": "https://play.google.com/store/apps/details?id=io.anygood"
    },
    "web": {
      "favicon": "./assets/drawable/ic_settings.png"
    },
    "extra": {
      "eas": {
        "projectId": "158931c0-f1fb-4ebb-8f7e-6e46867620c4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/158931c0-f1fb-4ebb-8f7e-6e46867620c4"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
